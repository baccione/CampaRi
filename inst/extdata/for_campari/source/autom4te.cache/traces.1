      AC_MSG_RESULT([yes (version $[NETCDF_FVERSION])])
      NETCDF4_FC=$(eval $NF_CONFIG --fc | $AWK '{print $[]1}')
      if test "$NETCDF4_FC" = "ccache"; then
        NETCDF4_FC=$(eval $NF_CONFIG --fc | $AWK '{print $[]2}')
