    AC_SUBST([NETCDF4_FC])
    AC_SUBST([NETCDF4_FFLAGS])
    AC_SUBST([NETCDF4_FLIBS])
    AC_DEFINE([HAVE_NETCDF4], [1], [Defined if you have NETCDF4 support])
  fi
fi
